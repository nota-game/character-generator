		"@types/pako": "^2.0.0",
		"@types/ungap__structured-clone": "^0.3.0",
		"@types/uuid": "^9.0.0",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"@ungap/structured-clone": "^1.0.1",
		"d3-scale": "^4.0.2",
		"base64-uint8": "^0.0.1",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"mathjs": "^10.5.3",
		"node-fetch": "^3.3.0",

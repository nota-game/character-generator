		"svelte-check": "^2.9.2",
		"ts-deep-equal": "^1.1.5",
		"ts-node": "^10.9.1",
		"tslib": "^2.4.1",
		"typescript": "^4.9.3",
		"uuid": "^9.0.0",
		"vite": "^4.0.0",
